{
  "name": "@mehotkhan/zcalendar",
  "type": "module",
  "version": "1.0.2",
  "description": "Calendar Components for Vue.js 3 using Tailwind CSS and Luxon",
  "author": {
    "name": "ali Zemani",
    "email": "sallam@alizemani.ir",
    "url": "https://github.com/mehotkhan"
  },
  "homepage": "https://mehotkhan.github.io/zcalendar/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mehotkhan/zcalendar.git"
  },
  "bugs": {
    "url": "https://github.com/mehotkhan/zcalendar/issues",
    "email": "sallam@alizemani.ir"
  },
  "keywords": [
    "vue-calendar",
    "tailwind-datepicker"
  ],
  "exports": {
    ".": {
      "import": "./dist/zcalendar.js",
      "require": "./dist/zcalendar.umd.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/zcalendar.umd.cjs",
  "module": "./dist/zcalendar.js",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "contributors": [],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "typecheck": "vue-tsc --noEmit",
    "commit": "cz",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": ":sparkles: Features",
                "hidden": false
              },
              {
                "type": "fix",
                "section": ":bug: Fixes",
                "hidden": false
              },
              {
                "type": "docs",
                "section": ":memo: Documentation",
                "hidden": false
              },
              {
                "type": "style",
                "section": ":barber: Code-style",
                "hidden": false
              },
              {
                "type": "refactor",
                "section": ":zap: Refactor",
                "hidden": false
              },
              {
                "type": "perf",
                "section": ":fast_forward: Performance",
                "hidden": false
              },
              {
                "type": "test",
                "section": ":white_check_mark: Tests",
                "hidden": false
              },
              {
                "type": "ci",
                "section": ":repeat: CI",
                "hidden": false
              },
              {
                "type": "chore",
                "section": ":repeat: Chore",
                "hidden": false
              }
            ]
          }
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": []
        }
      ]
    ]
  },
  "peerDependencies": {
    "luxon": "^1.11.10",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@vueuse/core": "^10.8.0",
    "vite-plugin-css-injected-by-js": "^3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@headlessui/vue": "^1.7.19",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/fs-extra": "^11.0.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.56.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "luxon": "^3.4.4",
    "postcss": "^8.4.35",
    "postcss-prefix-selector": "^1.16.0",
    "postcss-preset-env": "^9.4.0",
    "semantic-release": "^23.0.2",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "vite-plugin-dts": "3.7.3",
    "vitepress": "1.0.0-rc.44",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}